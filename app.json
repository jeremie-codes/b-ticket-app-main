{
  "expo": {
    "name": "B-tickets",
    "slug": "b-tickets",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-app.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "jsEngine": "jsc",
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/montserrat/400Regular/Montserrat_400Regular.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#240233",
          "image": "./assets/images/logo.png",
          "resizeMode": "contain",
          "imageWidht": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ab24ffc8-d887-43a0-94bd-d000311dcf08"
      }
    },
    "owner": "jey-mianda",
    "android": {
      "package": "com.jeremiemianda.btickets",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    }
  }
}
